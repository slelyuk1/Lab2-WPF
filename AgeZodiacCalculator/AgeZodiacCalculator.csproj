<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <OutputType>WinExe</OutputType>
        <TargetFramework>net5.0-windows</TargetFramework>
        <UseWPF>true</UseWPF>
        <RootNamespace>AgeZodiacCalculator</RootNamespace>
    </PropertyGroup>

    <ItemGroup>
      <Page Update="View\PickDateView.xaml">
        <SubType>Designer</SubType>
      </Page>
      <Page Update="ContentWindow.xaml">
        <SubType>Designer</SubType>
      </Page>
    </ItemGroup>

    <ItemGroup>
      <Compile Update="Window\ContentWindow.xaml.cs">
        <DependentUpon>ContentWindow.xaml</DependentUpon>
        <SubType>Code</SubType>
      </Compile>
      <Compile Update="ContentWindow.xaml.cs">
        <DependentUpon>ContentWindow.xaml</DependentUpon>
        <SubType>Code</SubType>
      </Compile>
    </ItemGroup>

</Project>
